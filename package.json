    "seedrandom": "^2.4.4",
    "uuid": "^3.3.2"
  },
  "devDependencies": {
    "@istanbuljs/nyc-config-typescript": "^0.1.3",
    "@types/chai": "^4.1.7",
    "@types/dotenv": "^6.1.1",
    "@types/mocha": "^5.2.7",
    "@types/mongodb": "^3.1.28",
    "@types/node": "^10.14.9",
    "@types/uuid": "^3.4.4",
    "chai": "^4.2.0",
    "dotenv": "^6.2.0",
    "mocha": "^5.2.0",
    "mongodb": "^3.2.7",
    "nyc": "^14.1.1",
    "prettier": "^1.18.2",
    "source-map-support": "^0.5.12",
    "ts-node": "^8.3.0",
    "tslint": "^5.17.0",
    "tslint-config-prettier": "^1.18.0",
    "typescript": "^3.5.2"
  }
