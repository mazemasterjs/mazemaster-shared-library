  "license": "MIT",
  "bugs": {
    "url": "https://github.com/mazemasterjs/shared-library/issues"
  },
  "homepage": "https://github.com/mazemasterjs/shared-library",
  "dependencies": {
    "@mazemasterjs/logger": "^1.0.10",
    "@types/object-hash": "^1.3.0",
    "@types/seedrandom": "^2.4.28",
